{
    "version": "3",
    "templates": [
      {
        "id": 1,
        "type": 1,
        "title": "Nginx Web Server",
        "description": "Simple and efficient web server",
        "categories": ["web", "server"],
        "platform": "linux",
        "logo": "https://portainer-io-assets.sfo2.digitaloceanspaces.com/logos/nginx.png",
        "image": "nginx:latest",
        "ports": ["8080:80/tcp"],
        "volumes": [
          {
            "container": "/usr/share/nginx/html"
          }
        ]
      },
      {
        "id": 2,
        "type": 1,
        "title": "Nginx Web Server (Custom HTML)",
        "description": "Nginx server with custom HTML mounted from a volume",
        "categories": ["web", "server"],
        "platform": "linux",
        "logo": "https://portainer-io-assets.sfo2.digitaloceanspaces.com/logos/nginx.png",
        "image": "nginx:latest",
        "ports": ["8080:80/tcp"],
        "volumes": [
          {
            "container": "/usr/share/nginx/html",
            "host": "/path/to/your/html"
          }
        ]
      }
    ]
  }
  